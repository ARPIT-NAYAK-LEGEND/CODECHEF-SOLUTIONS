int main(){
	int t,a,b,d;
	cin >> t;
	while(t--){
		cin >> a >> b;
		d = a-b;
		cout << d << endl;
	}
}

// CODECHEF BEGINEER 2 PROBLEM

int main(){
	int t,n,m,t1,t2,t3;
	cin >> t;
	while(t--){
		cin >> n >> m;
		t1 = n*2;
		t2 = m*4;
		t3 = t1+t2;
		cout << t3 << endl;
	}
}


